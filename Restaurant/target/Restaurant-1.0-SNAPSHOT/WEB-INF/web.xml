<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
         version="6.0">
    <servlet>
        <servlet-name>DishServlet</servlet-name>
        <servlet-class>servlet.DishServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DishServlet</servlet-name>
        <url-pattern>/Staff/DishServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>TableServlet</servlet-name>
        <servlet-class>servlet.TableServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>TableServlet</servlet-name>
        <url-pattern>/Customer/TableServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ReservationServlet</servlet-name>
        <servlet-class>servlet.ReservationServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ReservationServlet</servlet-name>
        <url-pattern>/Customer/ReservationServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>servlet.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/User/UserServlet</url-pattern>
    </servlet-mapping>

    
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
